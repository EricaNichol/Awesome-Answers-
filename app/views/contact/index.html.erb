<h1>My Contact Form</h1>

<%= form_tag "/contact" do %>
<div>
  <%= label_tag :name %>
  <%= text_field_tag :name %>
</div>
<div>
  <%= label_tag :email %>
  <%= email_field_tag :email %>
</div>
  <%= submit_tag "Contact Us"  %>
<% end %>




